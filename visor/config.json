{
	"name": "visor",
	"version": "9.0",
	"slug": "visor",
	"description": "A Home Assistant add-on that analyzes show cams .",
	"uart": true,
	"url": "https://github.com/julipache/addons/tree/main/visor",
	"startup": "application",
	"services": [
		"mqtt:need"
	],
	"arch": [
		"aarch64",
		"amd64",
		"armhf",
		"armv7",
		"i386"
	],
	"boot": "auto",
	"init": false,
	"ingress": true,
	"timeout": 30,
	"panel_icon": "mdi:zigbee",
	"ingress_port": 8099,
	"map": [
		"share:rw",
		"config:rw",
		"media:rw"
	],
	"ports": {
		"8099/tcp": 8099
	},
  "options": {
    "rtsp_url_1": "rtsp://usuario:contraseña@ip_del_dispositivo:puerto/stream1",
    "rtsp_url_2": "rtsp://usuario:contraseña@ip_del_dispositivo:puerto/stream2"
  },
  "schema": {
    "rtsp_url_1": "str",
    "rtsp_url_2": "str"
  }
}